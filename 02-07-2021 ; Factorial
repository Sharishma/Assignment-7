import java.util.*;
public class Factorial
{  
public static void main(String args[])
{  

Scanner s = new Scanner(System.in);
System.out.print("Enter the values ");

int a[] = new int[100]; //constrain
int N = 1 ;

int n = s.nextInt();
for(int i=0;i<n;i++)
{
a[i] = s.nextInt(); 
}

for(int i=0;i<n;i++)
{
    N=1;
    for(int j=1;j<=a[i];j++)
    {
        N= N* j;
    }
    System.out.println(N);
    
}
}
}


Output:
Enter the values 4
1
5
2
3
1
120
2
6
